{
  "name": "mirko/newsletter",
  "type": "typo3-cms-extension",
  "description": "TYPO3 extension to send any pages as Newsletter and provide statistics on opened emails and clicked links.",
  "homepage": "https://github.com/Mirko-Solutions/newsletter",
  "license": "GPL-2.0+",
  "version": "5.0.2",
  "keywords": [
    "TYPO3 CMS",
    "TYPO3",
    "newsletter",
    "email",
    "statistics"
  ],
  "require": {
    "php": ">=7.4.0",
    "ext-dom": "*",
    "ext-openssl": "*",
    "typo3/cms-backend": "^11.5 | ^10.4",
    "typo3/cms-core": "^11.5 | ^10.4",
    "typo3/cms-extbase": "^11.5 | ^10.4",
    "typo3/cms-fluid": "^11.5 | ^10.4",
    "typo3/cms-scheduler": "^11.5 | ^10.4"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "Mirko\\Newsletter\\": "Classes/",
      "Mirko\\Newsletter\\ThirdParty\\": "3dparty/",
      "Mirko\\Newsletter\\Tests\\": "Tests/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mirko\\Newsletter\\Tests\\": "Tests/"
    }
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": "web",
      "extension-key": "newsletter"
    }
  }
}
