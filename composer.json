{
  "name": "mirko/newsletter",
  "type": "typo3-cms-extension",
  "description": "TYPO3 extension to send any pages as Newsletter and provide statistics on opened emails and clicked links.",
  "homepage": "https://github.com/Mirko-Solutions/newsletter",
  "license": "GPL-2.0+",
  "version": "4.0.1",
  "keywords": [
    "TYPO3 CMS",
    "TYPO3",
    "newsletter",
    "email",
    "statistics"
  ],
  "require": {
    "php": ">=7.2.0",
    "ext-dom": "*",
    "ext-openssl": "*",
    "friendsoftypo3/typo3db-legacy": ">=1.1.2",
    "typo3/cms-backend": "^10.4 || ^11.5",
    "typo3/cms-core": "^10.4 || ^11.5",
    "typo3/cms-extbase": "^10.4 || ^11.5",
    "typo3/cms-fluid": "^10.4 || ^11.5",
    "typo3/cms-scheduler": "^10.4 || ^11.5"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "Mirko\\Newsletter\\": "Classes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mirko\\Newsletter\\Tests\\": "Tests/"
    }
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": "web"
    }
  }
}
